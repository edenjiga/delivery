{
  "expo": {
    "name": "Kangaroo Delivery",
    "slug": "kangaroo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kangaroo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.kangaroo.delivery.mobile",
      "config": {
        "googleMapsApiKey": "AIzaSyB-TJ5Pl7SQ59Q_E_L4Y_iKoVchji8GNHM"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses the location to suggest an address to the user."
      },
      "supportsTablet": true,
      "buildNumber": "1"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB-TJ5Pl7SQ59Q_E_L4Y_iKoVchji8GNHM"
        }
      },
      "package": "com.kangaroo.delivery.mobile",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/images/kangaroo_profile.png"
    }
  }
}
